{
  "name": "@tai-kun/surrealdb",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "The SurrealDB SDK for JavaScript.",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "tyes": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./clients": {
      "tyes": "./dist/clients.d.ts",
      "import": "./dist/clients.mjs",
      "require": "./dist/clients.cjs"
    },
    "./engines": {
      "tyes": "./dist/engines.d.ts",
      "import": "./dist/engines.mjs",
      "require": "./dist/engines.cjs"
    },
    "./formatters": {
      "tyes": "./dist/formatters.d.ts",
      "import": "./dist/formatters.mjs",
      "require": "./dist/formatters.cjs"
    },
    "./validators": {
      "tyes": "./dist/validators.d.ts",
      "import": "./dist/validators.mjs",
      "require": "./dist/validators.cjs"
    },
    "./errors": {
      "tyes": "./dist/errors.d.ts",
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.14.2",
    "@types/unzipper": "^0.10.9",
    "dprint": "^0.46.2",
    "esbuild": "^0.21.4",
    "tar": "^7.2.0",
    "type-fest": "^4.20.0",
    "typescript": "^5.4.5",
    "unzipper": "^0.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tai-kun/surrealdb-js.git"
  },
  "author": {
    "name": "tai-kun",
    "url": "https://github.com/tai-kun/"
  },
  "scripts": {
    "build": "bash scripts/build/source.sh",
    "check": "tsc --noEmit && dprint check",
    "test:bun:small": "node scripts/test.js ci bun-small-tests",
    "test:deno:small": "echo 'Deno tests are not yet implemented.' && exit 1",
    "test:node:small": "node scripts/test.js ci node-small-tests",
    "test:chrome:small": "node scripts/test.js ci chrome-small-tests",
    "test:firefox:small": "node scripts/test.js ci firefox-small-tests"
  },
  "volta": {
    "node": "22.2.0"
  }
}
