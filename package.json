{
  "name": "@tai-kun/surreal",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Javascript driver for SurrealDB",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "tyes": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./clients/standard": {
      "tyes": "./dist/clients/standard.d.ts",
      "import": "./dist/clients/standard.mjs",
      "require": "./dist/clients/standard.cjs"
    },
    "./clients/tiny": {
      "tyes": "./dist/clients/tiny.d.ts",
      "import": "./dist/clients/tiny.mjs",
      "require": "./dist/clients/tiny.cjs"
    },
    "./clients/full": {
      "tyes": "./dist/clients/full.d.ts",
      "import": "./dist/clients/full.mjs",
      "require": "./dist/clients/full.cjs"
    },
    "./values/standard": {
      "tyes": "./dist/values/standard/index.d.ts",
      "import": "./dist/values/standard/index.mjs",
      "require": "./dist/values/standard/index.cjs"
    },
    "./values/tiny": {
      "tyes": "./dist/values/tiny/index.d.ts",
      "import": "./dist/values/tiny/index.mjs",
      "require": "./dist/values/tiny/index.cjs"
    },
    "./values/full": {
      "tyes": "./dist/values/full/index.d.ts",
      "import": "./dist/values/full/index.mjs",
      "require": "./dist/values/full/index.cjs"
    },
    "./engines": {
      "tyes": "./dist/engines/index.d.ts",
      "import": "./dist/engines/index.mjs",
      "require": "./dist/engines/index.cjs"
    },
    "./formatters": {
      "tyes": "./dist/formatters/index.d.ts",
      "import": "./dist/formatters/index.mjs",
      "require": "./dist/formatters/index.cjs"
    },
    "./validators": {
      "tyes": "./dist/validators/index.d.ts",
      "import": "./dist/validators/index.mjs",
      "require": "./dist/validators/index.cjs"
    },
    "./errors": {
      "tyes": "./dist/errors.d.ts",
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.cjs"
    },
    "./_lib": {
      "tyes": "./dist/_lib/index.d.ts",
      "import": "./dist/_lib/index.mjs",
      "require": "./dist/_lib/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "is-plain-obj": "^4.1.0"
  },
  "optionalDependencies": {
    "cbor-redux": "^1.0.0",
    "decimal.js": "^10.4.3",
    "decimal.js-light": "^2.5.1",
    "zod": "^3.23.8",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.14.9",
    "@types/unzipper": "^0.10.9",
    "@types/ws": "^8.5.10",
    "dprint": "^0.46.3",
    "esbuild": "^0.21.5",
    "size-limit": "^11.1.4",
    "tar": "^7.4.0",
    "type-fest": "^4.20.1",
    "typedoc": "^0.26.3",
    "typescript": "^5.5.2",
    "unzipper": "^0.12.1",
    "uuid": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tai-kun/surreal.js.git"
  },
  "author": {
    "name": "tai-kun",
    "url": "https://github.com/tai-kun/"
  },
  "scripts": {
    "build": "node scripts/build/source.js && tsc -p tsconfig.build.json",
    "check": "tsc --noEmit && dprint check",
    "test:sm:bun": "node scripts/test.js bun small",
    "test:md:bun": "node scripts/test.js bun medium",
    "test:sm:deno": "node scripts/test.js deno small",
    "test:md:deno": "node scripts/test.js deno medium",
    "test:sm:node": "node scripts/test.js nodejs small",
    "test:md:node": "node scripts/test.js nodejs medium",
    "test:sm:chrome": "node scripts/test.js chrome small",
    "test:md:chrome": "node scripts/test.js chrome medium",
    "test:sm:webkit": "node scripts/test.js webkit small",
    "test:md:webkit": "node scripts/test.js webkit medium",
    "test:sm:firefox": "node scripts/test.js firefox small",
    "test:md:firefox": "node scripts/test.js firefox medium",
    "docs": "typedoc src src/clients/full.ts src/clients/standard.ts src/clients/tiny.ts src/values/full src/values/standard src/values/tiny src/engines src/formatters src/validators src/errors.ts --out docs --lang jp",
    "size": "npm run build && size-limit"
  },
  "size-limit": [
    {
      "import": {
        "dist/index.mjs": "{ initSurreal }",
        "dist/engines/index.mjs": "{ createHttpEngine }",
        "dist/formatters/index.mjs": "{ jsonFormatter }",
        "dist/validators/index.mjs": "{ emptyValidator }",
        "dist/clients/standard.mjs": "Client"
      },
      "gzip": true
    },
    {
      "import": {
        "dist/index.mjs": "{ initSurreal }",
        "dist/engines/index.mjs": "{ createHttpEngine }",
        "dist/formatters/index.mjs": "{ createCborFormatter }",
        "dist/validators/index.mjs": "{ emptyValidator }",
        "dist/clients/standard.mjs": "Client",
        "dist/values/tiny/index.mjs": "{ Datetime, Table, Thing, Uuid, Decimal, Duration, GeometryPoint, GeometryLine, GeometryPolygon, GeometryMultiPoint, GeometryMultiLine, GeometryMultiPolygon, GeometryCollection }"
      },
      "gzip": true
    },
    {
      "import": {
        "dist/index.mjs": "{ initSurreal }",
        "dist/engines/index.mjs": "{ createHttpEngine, createWebSocketEngine }",
        "dist/formatters/index.mjs": "{ createCborFormatter }",
        "dist/validators/index.mjs": "{ zodValidator }",
        "dist/clients/full.mjs": "Client",
        "dist/values/full/index.mjs": "{ Datetime, Table, Thing, Uuid, Decimal, Duration, GeometryPoint, GeometryLine, GeometryPolygon, GeometryMultiPoint, GeometryMultiLine, GeometryMultiPolygon, GeometryCollection }"
      },
      "gzip": true
    }
  ],
  "volta": {
    "node": "22.2.0"
  }
}
